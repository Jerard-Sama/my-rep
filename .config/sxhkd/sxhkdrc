super + w
	$BROWSER

super + Return
	$TERMINAL

super + s
	$HOME/scripts/dmenu-scripts/toggle_scratchbad.sh

super + {equal, minus, BackSpace}
	$HOME/scripts/dmenu-scripts/volume.sh {up, down, toggle}

super + m
	$HOME/scripts/dmenu-scripts/mount.sh

super + p
	$HOME/scripts/dmenu-scripts/passmenu.sh

super + x
	$HOME/scripts/dmenu-scripts/x.sh

super + shift + BackSpace
	$HOME/scripts/dmenu-scripts/exit.sh

super + o
	dmenu_run

super + shift + W
	$HOME/scripts/dmenu-scripts/set_wallpaper.sh
